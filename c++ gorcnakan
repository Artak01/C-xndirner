#include <iostream>

int main()
{
    int n = 0;
    int m = 0;
    std::cout << "n = ";
    std::cin >> n;
    std::cout << "m = ";
    std::cin >> m;
    
    int** mat = new int*[n];
    for(int i = 0; i < n; ++i){
        mat[i] = new int[m];
    }
    
    for(int i = 0; i < n; ++i){
        for(int j = 0; j < n; ++j){
            if(i %2 == 0){
                mat[i][j] = j+1;
            }
            else{
                mat[i][j] = n-j;
            }
        }
    }
    
    for(int i = 0; i < n; ++i){
        for(int j = 0; j < n; ++j){
          std::cout << mat[i][j] << " ";  
        }
        std::cout << "\n";
    }
    
    for(int i = 0; i < n; ++i){
       delete [] mat[i];
    }
    
    delete mat;
    return 0;
}
